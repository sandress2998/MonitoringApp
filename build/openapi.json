  "components": {
    "schemas": {
      "UpdatedBookRequest": {
        "type": "object",
        "description": "Обновление информации о пользователе",
        "properties": {
          "title": {
            "type": "string",
            "description": "Название книги",
            "example": "Преступление и наказание"
          },
          "author": {
            "type": "string",
            "description": "Имя автора",
            "example": "Федор Михайлович Достоевский"
          },
          "rating": {
            "type": "integer",
            "format": "int32",
            "description": "Рейтинг",
            "enum": [
              "1",
              "2",
              "3",
              "4",
              "5"
            ],
            "example": 5,
            "minimum": 0
          },
          "category": {
            "type": "string",
            "enum": [
              "NOVEL",
              "DETECTIVE",
              "FANTASY",
              "UNKNOWN"
            ],
            "example": "NOVEL"
          }
        },
        "required": [
          "author",
          "category",
          "rating",
          "title"
        ]
      },
      "Book": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "author": {
            "type": "string"
          },
          "rating": {
            "type": "integer",
            "format": "int32"
          },
          "category": {
            "type": "string",
            "enum": [
              "FANTASY",
              "NOVEL",
              "DETECTIVE",
              "UNKNOWN"
            ]
          },
          "id": {
            "type": "string",
            "format": "uuid",
            "readOnly": true
          },
          "createdAt": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          }
        },
        "required": [
          "author",
          "category",
          "createdAt",
          "rating",
          "title"
        ]
      },
      "CreateOrderRequest": {
        "type": "object",
        "properties": {
          "bookId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "required": [
          "bookId"
        ]
      },
      "Order": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "readOnly": true
          },
          "createdAt": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          },
          "book": {
            "$ref": "#/components/schemas/Book"
          },
          "status": {
            "type": "string",
            "enum": [
              "DONE",
              "UNDONE"
            ]
          }
        },
        "required": [
          "book",
          "createdAt",
          "status"
        ]
      }
    },
    "securitySchemes": {
      "JWT": {
        "type": "http",
        "scheme": "bearer",
